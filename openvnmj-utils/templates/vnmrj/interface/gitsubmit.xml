<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="ProcPlotBasic" element="pages" type="processing" >
  <group size="510 310"
    style="Heading1"
    label="Basic"
    vq="personaType"
    show="$S=1 PersonaManager('showInPanel','process','basic'):$S $SHOW=$S"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="ProcPlotBasic"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    rows="1"
    columns="1"
    >
    <group loc="5 0" size="490 255"
      style="Heading1"
      label="OpenVnmrJ submission"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="proc1d-ez"
      useref="no"
      subtype="Titled"
      expanded="yes"
      rows="1"
      columns="1"
      >
      <textfile loc="10 125" size="450 105"
        style="PlainText"
        vq="gitDir"
        set="$VALUE=gitDir+'/README.md'"
        editable="yes"
        wrap="yes"
        units="char"
        />
      <label loc="10 105" size="90 20"
        style="Label1"
        label="Readme:"
        />
      <entry loc="200 65" size="260 20"
        style="PlainText"
        vq="gitName"
        vc="gitName='$VALUE'"
        set="$VALUE=gitName"
        disable="Label"
        />
      <entry loc="200 45" size="260 20"
        style="PlainText"
        vq="gitUser"
        vc="gitUser='$VALUE'"
        set="$VALUE=gitUser"
        disable="Label"
        />
      <entry loc="200 85" size="260 20"
        style="PlainText"
        vq="gitTitle"
        vc="gitTitle='$VALUE'"
        set="$VALUE=gitTitle"
        disable="Label"
        />
      <entry loc="200 25" size="260 20"
        style="PlainText"
        vq="gitDir"
        vc="gitDir='$VALUE'"
        set="$VALUE=gitDir"
        disable="Label"
        />
      <label loc="10 85" size="190 20"
        style="Label1"
        label="Title"
        />
      <label loc="10 65" size="190 20"
        style="Label1"
        label="Submission name (single word)"
        />
      <label loc="10 25" size="190 20"
        style="Label1"
        label="Path to appdir"
        />
      <label loc="10 45" size="190 20"
        style="Label1"
        label="Github user name"
        />
    </group>
  </group>
</template>
