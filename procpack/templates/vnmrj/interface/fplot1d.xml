<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="fplot1d" element="pages" type="processing" >
  <group size="515 515"
    style="PlainText"
    label="Fancy 1D Plot"
    bg="transparent"
    border="Etched"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="fplot1d"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    >
    <group loc="5 130" size="250 145"
      style="Heading3"
      label="Main Spectrum"
      bg="transparent"
      border="Etched"
      tab="no"
      enable="no"
      reference="MainSpectrum"
      useref="no"
      subtype="Titled"
      expanded="yes"
      >
      <group loc="5 75" size="245 40"
        label="Region for auto vert scale"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        reference="SWselector"
        useref="no"
        subtype="Untitled"
        expanded="yes"
        >
        <button loc="25 20" size="45 20"
          style="Label3"
          label="Select"
          vq="fplot1dFVsp fplot1dFVwp"
          vc="if crmode='c' then write('error','Use 2 cursors') else fplot1d('selectMainVert') endif ds('restart')"
          bg="VJBackground"
          decor1="yes"
          tooltip="Select SW from cursors"
          />
        <label loc="70 20" size="15 20"
          style="Label1"
          label="or"
          justify="Right"
          />
        <label loc="15 0" size="230 20"
          style="Label1"
          label="Region to use for auto vert. scale:"
          justify="Left"
          />
        <entry loc="90 20" size="50 20"
          style="StdPar"
          vq="fplot1dFVsp fplot1dFVwp"
          vc="fplot1d('conv2ppm',fplot1dFVsp,fplot1dFVwp):$ppm1,$ppm2 fplot1d('convFromppm',$VALUE,$ppm2):fplot1dFVsp,fplot1dFVwp"
          set="fplot1d('conv2ppm',fplot1dFVsp,fplot1dFVwp):$VALUE,$ppm2"
          digits="1"
          tooltip="SW to use for vsadj"
          />
        <label loc="140 20" size="20 20"
          style="Label1"
          label="to"
          justify="Center"
          />
        <entry loc="160 20" size="50 20"
          style="StdPar"
          vq="fplot1dFVsp fplot1dFVwp"
          vc="fplot1d('conv2ppm',fplot1dFVsp,fplot1dFVwp):$ppm1,$ppm2 fplot1d('convFromppm',$ppm1,$VALUE):fplot1dFVsp,fplot1dFVwp"
          set="fplot1d('conv2ppm',fplot1dFVsp,fplot1dFVwp):$ppm1,$VALUE"
          digits="1"
          tooltip="SW to use for vsadj"
          />
        <label loc="210 20" size="35 20"
          style="Label1"
          label="ppm"
          justify="Center"
          />
      </group>
      <group loc="5 120" size="245 20"
        label="Adjust Vert Scale"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        >
        <label loc="35 0" size="140 20"
          style="Label1"
          label="Adjust vertical scale by"
          justify="Right"
          />
        <label loc="215 0" size="15 20"
          style="Label1"
          label="%"
          justify="Center"
          />
        <entry loc="180 0" size="30 20"
          style="StdPar"
          vq="fplot1dFVadj"
          vc="fplot1dFVadj=$VALUE/100"
          set="$VALUE=fplot1dFVadj*100"
          digits="0"
          tooltip="Adjust the vs by this amount"
          />
      </group>
      <group loc="5 25" size="245 40"
        label="Region to Plot"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        reference="SWselector"
        useref="no"
        subtype="Untitled"
        expanded="yes"
        >
        <button loc="25 20" size="45 20"
          style="Label3"
          label="Select"
          vq="fplot1dFsp fplot1dFwp fplot1dIRsp fplot1dIRwp"
          vc="if crmode='c' then write('error','Use 2 cursors') else fplot1d('selectMain') endif ds('restart')"
          bg="VJBackground"
          decor1="yes"
          tooltip="Select SW from cursors"
          />
        <label loc="70 20" size="15 20"
          style="Label1"
          label="or"
          justify="Right"
          />
        <label loc="15 0" size="230 20"
          style="Label1"
          label="Region to plot:"
          justify="Left"
          />
        <entry loc="90 20" size="50 20"
          style="StdPar"
          vq="fplot1dFsp fplot1dFwp fplot1dIRsp fplot1dIRwp"
          vc="fplot1d('conv2ppm',fplot1dFsp,fplot1dFwp):$ppm1,$ppm2 fplot1d('convFromppm',$VALUE,$ppm2):fplot1dFsp,fplot1dFwp fplot1d('fixHorizReg')"
