          set="fplot1d('conv2ppm',fplot1dFsp,fplot1dFwp):$ppm1,$VALUE"
          digits="1"
          tooltip="SW for the main plot"
          />
        <label loc="210 20" size="35 20"
          style="Label1"
          label="ppm"
          justify="Center"
          />
